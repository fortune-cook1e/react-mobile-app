{
	"name": "react-mobile-app",
	"version": "1.0.0",
	"description": "react-mobile-app",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
		"start": "cross-env vite",
		"build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
		"lint": "cross-env eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
		"stylelint": "stylelint src *.less,*.css --fix",
		"commit": "cz",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx}": [
			"prettier --write",
			"yarn lint",
			"git add ."
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fortune-cook1e/react-mobile-app.git"
	},
	"author": "fortune_cookie",
	"license": "ISC",
	"keywords": [
		"react"
	],
	"bugs": {
		"url": "https://github.com/fortune-cook1e/react-mobile-app/issues"
	},
	"homepage": "https://github.com/fortune-cook1e/react-mobile-app#readme",
	"dependencies": {
		"@fe-cookie/render-engine": "^1.0.0",
		"ahooks": "^3.7.1",
		"antd": "^4.23.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-error-boundary": "^3.1.4",
		"react-router-dom": "^6.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@babel/plugin-transform-runtime": "^7.19.1",
		"@babel/preset-env": "^7.19.1",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
		"@types/react": "^17.0.9",
		"@types/react-dom": "^17.0.6",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"@vitejs/plugin-react": "^2.1.0",
		"babel-loader": "^8.2.5",
		"clean-webpack-plugin": "^4.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^4.1.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.8",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-webpack-plugin": "^5.5.0",
		"less": "^4.1.3",
		"less-loader": "^11.0.0",
		"mini-css-extract-plugin": "^2.6.1",
		"postcss": "^8.4.16",
		"postcss-import": "^15.0.0",
		"postcss-loader": "^7.0.1",
		"postcss-preset-env": "^7.8.2",
		"prettier": "^2.7.1",
		"react-refresh": "^0.14.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.12.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^28.0.0",
